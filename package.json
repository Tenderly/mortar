{
  "name": "mortar-tenderly",
  "description": "Solidity IaC deployment tool",
  "version": "0.0.0",
  "author": "Tenderly",
  "bin": {
    "mortar": "./bin/run"
  },
  "bugs": "https://github.com/Tenderly/mortar-tenderly/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "cli-ux": "^5.5.1",
